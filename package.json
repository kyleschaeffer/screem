{
  "name": "screem",
  "version": "1.0.0",
  "description": "Screen measurement and device diagnostics",
  "main": "src/app.js",
  "scripts": {
    "dev": "yarn run clean && parcel src/index.html --open --out-dir docs",
    "build": "yarn run clean && parcel build src/index.html --out-dir docs",
    "clean": "rm -f docs/app.* && rm -rf .cache",
    "scrape": "node --use_strict scrape/scrape.js"
  },
  "repository": "git@github.com:kyleschaeffer/screem.git",
  "author": "@kyleschaeffer",
  "license": "MIT",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "vue": "^2.6.7"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "3.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.17.1",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.7"
  }
}
